void plotNodelay (uint8_t x, uint8_t y, uint8_t value) { //Control LED intput : x , y , LED status
uint8_t addressplot;
if (x >= 0 && x <= 7) { addressplot = 3; }
if (x >= 8 && x <= 15) { addressplot = 2; x = x - 8; }
if (x >= 16 && x <= 23) { addressplot = 1; x = x - 16;}
if (x >= 24 && x <= 31) { addressplot = 0; x = x - 24;}
if (value == 1) {
lc.setLed(addressplot, y, x, true);
} else {
lc.setLed(addressplot, y, x, false);
}
}
/***************************************************************************************/
void print_tiny_char(uint8_t x, uint8_t y, char c)
{
uint8_t dots;
if (c >= 'A' && c <= 'Z' || (c >= 'a' && c <= 'z') ) { c &= 0x1F; } // A-Z maps to 1-26
else if (c >= '0' && c <= '9') { c = (c - '0') + 32; }
else if (c == ' ') { c = 0; } // space
else if (c == '.') { c = 27; } // full stop
else if (c == ':') { c = 28; } // colon
else if (c == '\''){ c = 29; } // single quote mark
else if (c == '!') { c = 30; } // single quote mark
else if (c == '?') { c = 31; } // single quote mark
for (uint8_t col = 0; col < 3; col++) {
dots = pgm_read_byte_near(&mytinyfont[c][col]);
for (uint8_t row = 0; row < 5; row++) {
if (dots & (16 >> row))
plotNodelay(x + col, y + row, 1);
else
plotNodelay(x + col, y + row, 0);
}
}
}
